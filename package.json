  "scripts": {
    "dev": "vite",
    "build": "vite build --mode production",
    "build:develop": "vite build --mode develop",
    "build:uat": "vite build --mode uat",
    "build:refine": "tsc && refine build",
    "preview": "refine start",
    "refine": "refine",
    "lint": "eslint --ext .ts,.tsx src --color",
    "husky-install": "npx husky install & npx husky add .husky/pre-commit 'yarn run lint-staged'",
    "format": "prettier --write \"src/**/*.{tsx,ts}\"",
    "test:watch": "jest test --watchAll",
    "test:coverage": "jest test --coverage"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn run lint",
      "yarn run format"
    ]
  }
}
